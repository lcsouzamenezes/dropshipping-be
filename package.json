{
  "name": "dropshipping",
  "version": "1.0.0",
  "description": "A dropshipping shared plataform",
  "main": "src/shared/infra/http/server.ts",
  "author": "Jonathan Bertoldi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "seed:generate": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create -c seed -n",
    "seed:revert": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli migration:revert -c seed",
    "seed:run": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -c seed",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --respawn ./src/shared/infra/http/server.ts",
    "dev:queue": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --transpile-only --respawn ./src/shared/infra/http/queue.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles"
  },
  "dependencies": {
    "@bull-board/express": "^3.6.0",
    "aws-sdk": "^2.1058.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.29.3",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.13.0",
    "handlebars": "^4.7.7",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "mysql": "^2.14.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.3",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.3.1",
    "tsyringe": "^4.6.0",
    "typeorm": "0.2.37",
    "uuid": "^8.3.2",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^8.0.29",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
